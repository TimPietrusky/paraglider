{
  "name": "paraglider",
  "version": "1.1.0",
  "description": "A Slider with parallax API",
  "author": {
    "name": "Gregor Adams",
    "email": "greg@pixelass.com",
    "homepage": "http://pixelass.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixelass/paraglider.git"
  },
  "keywords": [
    "parallax",
    "slider",
    "touch",
    "slideshow",
    "carousel",
    "animation",
    "effects",
    "custom",
    "easing",
    "autoplay",
    "mobile",
    "desktop",
    "ios",
    "android"
  ],
  "bugs": {
    "url": "https://github.com/pixelass/paraglider/issues"
  },
  "homepage": "https://github.com/pixelass/paraglider#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "babel": "cross-env NODE_ENV=production babel src --out-dir lib --copy-files",
    "build": "cross-env NODE_ENV=production babel-node ./scripts/build",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "dev": "babel-node ./scripts/dev",
    "dist": "cross-env NODE_ENV=production babel-node ./scripts/dist",
    "docs": "babel-node ./scripts/jsdoc && jsdoc -c jsdoc.json",
    "eslint": "xo '{demo,src}/**/*.js'",
    "lint": "npm run eslint; npm run stylelint",
    "prepush": "npm test",
    "prerelease": "npm run babel; npm test",
    "posttest": "npm run lint",
    "release": "standard-version --no-verify",
    "report": "nyc report --reporter=html",
    "server": "babel-node ./scripts/server",
    "start": "npm run build && npm run server",
    "stylelint": "stylelint {demo,src}/**/*.{css,less,scss}",
    "test": "nyc ava --tap | tap-nyan",
    "watch": "babel-node ./scripts/watch"
  },
  "config": {
    "devPort": 3000,
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  },
  "devDependencies": {
    "abcq": "^0.2.2",
    "ava": "^0.19.1",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-hmr": "^0.3.5",
    "bundle-collapser": "^1.2.1",
    "caniuse-db": "^1.0.30000670",
    "copy": "^0.3.0",
    "coveralls": "^2.11.16",
    "cross-env": "^5.0.0",
    "css-modulesify": "^0.28.0",
    "cz-conventional-changelog": "^2.0.0",
    "easing-utils": "^0.0.7",
    "errorify": "^0.3.1",
    "express": "^4.15.3",
    "git-repo-name": "^0.6.0",
    "git-user-email": "^0.2.2",
    "git-username": "^0.5.0",
    "globby": "^6.1.0",
    "hopsdoc": "^0.1.7",
    "husky": "^0.13.2",
    "ignore-styles": "^5.0.1",
    "jsdoc": "^3.4.3",
    "mkdirp": "^0.5.1",
    "nyc": "^11.0.1",
    "pify": "^3.0.0",
    "postcss-cssnext": "^2.11.0",
    "pug": "^2.0.0-rc.1",
    "rimraf": "^2.6.1",
    "sharp": "^0.18.1",
    "standard-version": "^4.0.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-no-unsupported-browser-features": "^1.0.0",
    "tap-nyan": "^1.1.0",
    "uglifyify": "^3.0.4",
    "watchify": "^3.9.0",
    "winston": "^2.3.1",
    "xo": "^0.18.2"
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 safari versions",
    "last 2 firefox versions",
    "last 2 edge versions",
    "last 2 ios versions",
    "last 2 chromeandroid versions"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "stage-2",
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "no-console": "error"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard"
    ],
    "plugins": [
      "stylelint-no-unsupported-browser-features"
    ],
    "rules": {
      "indentation": 2,
      "number-leading-zero": null,
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "plugin/no-unsupported-browser-features": [
        true
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "ava": {
    "files": [
      "src/**/spec/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "presets": [
      "@ava/stage-4",
      "@ava/transform-test-files"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  }
}
